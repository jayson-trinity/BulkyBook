@model IEnumerable<RegistrationModel>
@{
    ViewData["Title"] = "Index";
}
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Registered Users</h2>
        </div>
    </div>
</div>
<br />
<br />
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.FirstName
                </td>
                <td width="30%">
                    @obj.LastName
                </td>
                 <td width="30%">
                    @obj.Email
                </td>
                <td>
                    <div class="w-75 btn-group" role="group">
                        <a asp-controller="Admin" asp-action="Delete" asp-route-id="@obj.Id" class="btn btn-danger mx-2"><i class="bi bi-trash3"></i>Delete</a>
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>
